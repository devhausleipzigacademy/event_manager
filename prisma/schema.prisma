// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Room {
  id String @id @default(uuid())
  name String
  postal String
  address String
  city String
  description String
  images String[] @default([])
}

